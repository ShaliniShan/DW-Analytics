		private function getMaxTime($interactions) {
			$len = count($interactions) - 1;
			$max = -1;
			while($len) {
				$dateParseIn = $interactions[$len]->get("timeIn")->format("Y-m-d H:i:s");
				$formated_date = date('H', $dateParseIn);
				if($formated_date > $max) {
					$max = $formated_date;
				}
				$len--;
			}
			return $max;
		}

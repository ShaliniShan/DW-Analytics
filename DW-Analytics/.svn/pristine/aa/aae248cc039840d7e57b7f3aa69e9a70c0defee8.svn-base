#main-container {
	font-family: 'Source Sans Pro','Helvetica Neue',Helvetica,Arial,sans-serif;
}
svg {
	background-color: #444;
	/* margin-left: auto; */
	/* margin-right: auto; */
	display: block;
	margin-bottom: 30px;
	/* width: 100% !important; */
}
text {
	font-family: 'Source Sans Pro','Helvetica Neue',Helvetica,Arial,sans-serif;
	fill: #ececec;
}
#ajax_table {
	display: none;
}
#small_bar_desc {
	text-align: center;
	padding-bottom: 50px;
}
#content_header {
	font-weight: bold;
}	
.bar {
	fill: purple;
	shape-rendering: crispEdges;
}
.bar-label {
	font-family: 'Source Sans Pro','Helvetica Neue',Helvetica,Arial,sans-serif;
	fill: #ececec;
	text-anchor: middle;
	font-size: 13px;
}
.axis path, .axis line {
	fill: none;
	stroke: #333;
	shape-rendering: crispEdges;
}
.gridline path, .gridline line {
	fill: none;
	stroke: #333;
	shape-rendering: crispEdges;
}
.point {
	fill: #333;
}
.center {
	text-align: center;
}
.marginB {
	margin-top: 20px;
	margin-bottom: 20px;
}
.tick image {
	border-radius: 50px;
}
.bar_active {
	fill: #666 !important;
}

